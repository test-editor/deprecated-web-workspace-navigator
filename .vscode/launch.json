{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Chrome for Jasmine",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:9876/debug.html",
        "webRoot": "${workspaceRoot}/src",
        "sourceMaps": true,
        "userDataDir": "${workspaceRoot}/.vscode/chrome",
        "skipFiles": [
            "node_modules/**/*"
        ]
      }
    ]
  }
