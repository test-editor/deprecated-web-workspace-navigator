{
  "name": "@testeditor/workspace-navigator",
  "version": "0.28.0",
  "description": "Angular component for workspace handling and navigation.",
  "main": "./bundles/workspace-navigator.umd.js",
  "module": "./workspace-navigator.es5.js",
  "es2015": "./workspace-navigator.js",
  "typings": "./workspace-navigator.d.ts",
  "license": "MIT",
  "keywords": [
    "test-editor",
    "angular",
    "workspace"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/test-editor/web-workspace-navigator"
  },
  "contributors": [
    "Franz Becker",
    "Gunther Bachmann"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "clean": "rimraf out-tsc dist/*",
    "upgradeTesteditorOrgDependencies": "yarn upgrade @testeditor",
    "prebuild": "yarn run upgradeTesteditorOrgDependencies && yarn run clean",
    "build": "node build.js",
    "build-demo": "tsc -p src/demo/",
    "build-demo:watch": "tsc -p src/demo/ -w",
    "serve": "lite-server -c=bs-config.json",
    "prestart": "yarn run upgradeTesteditorOrgDependencies && yarn run build-demo",
    "start": "concurrently \"yarn run build-demo:watch\" \"yarn run serve\"",
    "build-test": "tsc -p src/lib/tsconfig.spec.json",
    "build-test:watch": "tsc -p src/lib/tsconfig.spec.json -w",
    "pretest": "yarn run upgradeTesteditorOrgDependencies && yarn run build-test",
    "test": "concurrently \"yarn run build-test:watch\" \"karma start karma.conf.js\"",
    "coverage": "karma start karma-coverage.conf.js --single-run",
    "pretest:once": "yarn run upgradeTesteditorOrgDependencies && yarn run build-test",
    "test:once": "karma start karma.conf.js --single-run",
    "preintegration": "yarn run upgradeTesteditorOrgDependencies && yarn run build && cd integration && yarn run clean && yarn install",
    "integration": "yarn run integration:aot && yarn run integration:jit",
    "integration:jit": "cd integration && yarn run e2e",
    "integration:aot": "cd integration && yarn run e2e:aot",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "postversion": "git push && git push --tags"
  },
  "peerDependencies": {
    "@angular/common": ">=4.0.0 <6.0.0",
    "@angular/core": ">=4.0.0 <6.0.0",
    "@angular/forms": ">=4.0.0 <6.0.0",
    "@testeditor/messaging-service": "^1.0.0",
    "bootstrap": ">=4.0.0"
  },
  "devDependencies": {
    "@angular/common": "5.2.10",
    "@angular/compiler": "5.2.10",
    "@angular/compiler-cli": "5.2.10",
    "@angular/core": "5.2.10",
    "@angular/forms": "5.2.10",
    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@testeditor/messaging-service": "~1.4.0",
    "@types/jasmine": "2.8.6",
    "@types/node": "9.6.6",
    "bootstrap": "4.1.0",
    "camelcase": "5.0.0",
    "concurrently": "3.5.1",
    "core-js": "2.5.5",
    "font-awesome": "4.7.0",
    "glob": "7.1.2",
    "jasmine-core": "3.1.0",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-html-reporter": "0.2.7",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "1.0.0",
    "karma-remap-coverage": "0.1.5",
    "lite-server": "2.3.0",
    "rimraf": "2.6.2",
    "rollup": "0.58.2",
    "rollup-plugin-commonjs": "9.1.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-uglify": "3.0.0",
    "rxjs": "5.5.10",
    "systemjs": "0.21.3",
    "ts-mockito": "2.3.0",
    "tslint": "5.9.1",
    "typescript": "2.8.3",
    "webdriver-manager": "^12.0.6",
    "yarn": "^1.6.0",
    "zone.js": "0.8.26"
  }
}
