{
  "name": "integration-test",
  "version": "1.0.0",
  "description": "App for integration tests",
  "scripts": {
    "clean": "rimraf aot/ dist/ node_modules/@testeditor/workspace-navigator/",
    "upgradeTesteditorOrgDependencies": "yarn upgrade @testeditor",
    "prebuild": "yarn run upgradeTesteditorOrgDependencies",
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "build:aot": "node build.js",
    "serve": "lite-server -c=bs-config.json",
    "serve:aot": "lite-server -c bs-config.aot.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "serve:e2e-aot": "lite-server -c bs-config.e2e-aot.json",
    "prestart": "yarn run upgradeTesteditorOrgDependencies && yarn run build",
    "start": "concurrently \"yarn run build:watch\" \"yarn run serve\"",
    "pree2e": "yarn run upgradeTesteditorOrgDependencies && yarn run build:e2e && yarn run build",
    "e2e": "concurrently \"yarn run serve:e2e\" \"yarn run protractor\" --kill-others --success first",
    "pree2e:aot": "yarn run upgradeTesteditorOrgDependencies && yarn run build:e2e && yarn run build:aot",
    "e2e:aot": "concurrently \"yarn run serve:e2e-aot\" \"yarn run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update --ignore_ssl --gecko=false",
    "protractor": "protractor protractor.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "5.2.10",
    "@angular/compiler": "5.2.10",
    "@angular/compiler-cli": "5.2.10",
    "@angular/core": "5.2.10",
    "@angular/forms": "5.2.10",
    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@testeditor": "file:../dist",
    "@testeditor/messaging-service": "^1.4.0",
    "@testeditor/testeditor-commons": "~0.1.4",
    "@testeditor/workspace-navigator": "file:../dist",
    "bootstrap": "4.1.0",
    "core-js": "2.5.5",
    "font-awesome": "4.7.0",
    "rxjs": "5.5.10",
    "systemjs": "0.21.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@types/jasmine": "2.8.6",
    "concurrently": "3.5.1",
    "glob": "7.1.2",
    "jasmine-core": "3.1.0",
    "lite-server": "2.3.0",
    "protractor": "5.3.1",
    "rimraf": "2.6.2",
    "rollup": "0.58.2",
    "rollup-plugin-commonjs": "9.1.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-uglify": "3.0.0",
    "typescript": "2.8.3",
    "webdriver-manager": "^12.0.6"
  },
  "repository": {}
}
